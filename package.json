{
  "name": "@entrolytics/svelte-sdk",
  "version": "2.0.0",
  "description": "SvelteKit integration for Entrolytics - First-party growth analytics for the edge",
  "keywords": [
    "entrolytics",
    "analytics",
    "svelte",
    "sveltekit",
    "web-analytics",
    "tracking"
  ],
  "author": "Entrolytics <hey@ng.entrolytics.click>",
  "license": "MIT",
  "homepage": "https://ng.entrolytics.click",
  "repository": {
    "type": "git",
    "url": "https://github.com/entrolytics/svelte-sdk",
    "directory": "entro-svelte"
  },
  "bugs": {
    "url": "https://github.com/entrolytics/svelte-sdk/issues"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "svelte": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=24.x"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "svelte": "^5.45.9",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "svelte": ">=5.0.0"
  },
  "svelte": "./dist/index.js"
}
